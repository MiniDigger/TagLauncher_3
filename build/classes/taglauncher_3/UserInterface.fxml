<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" style="" styleClass="main_scene" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="taglauncher_3.Controller">
    <children>
        <ToolBar layoutY="261.0" prefHeight="40.0" prefWidth="521.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <items>
                <Label id="status" fx:id="status" text="Status" />
            </items>
        </ToolBar>
            <ComboBox id="installversionList" fx:id="installversionList" layoutX="469.0" layoutY="508.0" onAction="#updateType" prefHeight="26.0" prefWidth="254.0" promptText="select version to install" />
            <Label layoutX="271.0" layoutY="511.0" text="Download version" />
            <CheckBox id="forceInst" fx:id="forceInst" layoutX="699.0" layoutY="512.0" mnemonicParsing="false" text="Force Download" />
            <Button id="installGame" fx:id="installGame" layoutX="477.0" layoutY="457.0" mnemonicParsing="false" onAction="#installGameBtn" text="Install Game" />
            <Label id="versionType" fx:id="versionType" layoutX="643.0" layoutY="447.0" text="Release - Beta - Alpha" />
            <Label layoutX="596.0" layoutY="447.0" text="Type" />
      <ToolBar prefHeight="25.0" prefWidth="807.0" styleClass="top_title_bar">
         <items>
            <Label text="The Primal Launcher" />
            <Pane prefHeight="16.0" prefWidth="639.0" />
            <Button fx:id="minimize" minHeight="18.0" minWidth="18.0" mnemonicParsing="false" onAction="#launchMinimize" prefHeight="18.0" prefWidth="18.0" styleClass="minimize_button">
               <font>
                  <Font size="9.0" />
               </font>
            </Button>
            <Button fx:id="exit" minHeight="18.0" minWidth="18.0" mnemonicParsing="false" onAction="#launchExit" prefHeight="18.0" prefWidth="18.0" styleClass="exit_button">
               <font>
                  <Font size="9.0" />
               </font>
            </Button>
         </items>
      </ToolBar>
      <VBox layoutX="279.0" layoutY="200.0" prefHeight="165.0" prefWidth="265.0" styleClass="main_login_area">
         <children>
            <Label text="Username">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextField fx:id="username" prefHeight="26.0" prefWidth="230.0" promptText="Username" styleClass="main_username" />
            <Pane prefHeight="20.0" prefWidth="267.0" />
            <Label text="Version">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <ComboBox id="version" fx:id="version" prefHeight="25.0" prefWidth="312.0" promptText="Select A Version" />
            <Pane prefHeight="20.0" prefWidth="267.0" />
            <HBox prefHeight="18.0" prefWidth="253.0">
               <children>
                  <Button fx:id="options" mnemonicParsing="false" onAction="#launchOptions" prefHeight="25.0" prefWidth="110.0" text="Options" />
                  <Pane prefHeight="25.0" prefWidth="46.0" />
                  <Button id="launch" fx:id="launch" mnemonicParsing="false" onAction="#lunachMineCraft" prefHeight="26.0" prefWidth="110.0" text="Launch" />
               </children>
            </HBox>
         </children>
      </VBox>
      <Pane layoutY="28.0" prefHeight="531.0" prefWidth="5.0" styleClass="top_title_bar" />
      <Pane layoutX="802.0" layoutY="28.0" prefHeight="531.0" prefWidth="5.0" styleClass="top_title_bar" />
    </children>
</AnchorPane>
